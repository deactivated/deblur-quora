{
  "manifest_version": 2,

  "name": "Click to Deblur Quora",
  "description": "",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://*.quora.com/*"],
      "js": ["jquery.js", "spin.min.js", "deblur.js"]
    }
  ],

  "permissions": [
    "*://*.quora.com/*"
  ],

  "web_accessible_resources": [
    "injected.js", "styles.css"
  ]
}
